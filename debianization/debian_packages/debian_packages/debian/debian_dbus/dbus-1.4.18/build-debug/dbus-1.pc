interfaces_dir=/usr/lib/x86_64-linux-gnu/dbus-1.0/debug-build/share/dbus-1/interfaces
daemondir=/usr/lib/x86_64-linux-gnu/dbus-1.0/debug-build/bin

Name: dbus
Description: Free desktop message bus
Version: 1.4.18
Libs: -L${libdir} -ldbus-1 -lpthread -lrt 
Cflags: -I${includedir}/dbus-1.0 -I${libdir}/dbus-1.0/include
